<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
		<security>
			<requestFiltering>
				<!--
				   10MB =  10485760 = 10* 1024 * 1024
				-->
				<requestLimits maxAllowedContentLength="10485760" />
			</requestFiltering>
		</security>
	</system.webServer>
</configuration>



